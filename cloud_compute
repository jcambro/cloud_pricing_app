#! /usr/bin/python2

#Created by John Ambrose

from Tkinter import *



my_font = ("Helvetica", 20)
major_font = ("Helvetica", 30, "bold")
minor_font = ("Helvetica", 26, "bold")

class Application(Tk):
	storage_needed = 0
	read_ops = 0
	write_ops = 0
	data_scan = 0
	data_return = 0

	def __init__(self, *args, **kwargs):
		Tk.__init__(self, *args, **kwargs)

		#Tkfont.
		#setup frame
		#container 2 is used to center the frames.
		self.container = Frame(self)
		self.container2 = Frame(self)
		self.container.config(width=1150, height=600)

		self.container.pack(side="top", fill="both", expand=True, padx=10, pady=10)
		self.container.grid_rowconfigure(0, weight=1)
		self.container.grid_columnconfigure(0, weight=1)
		self.container2.place(in_=self.container, anchor="c", relx=0.5, rely=0.5)

		self.frames = {}

		#puts all the frames in a nice container.
		for F in (StartPage, StoragePage, ComputingPage):
			frame = F(self.container2, self)
			self.frames[F] = frame
			frame.grid(row=0, column=0, sticky="NSEW")

		#Starts the application at the correct home page
		self.show_frame(StartPage)

	#need a function for this so a button can change pages
	def show_frame(self, context):
		frame = self.frames[context]
		frame.tkraise()

	def reset_frame(self, context):
		frame = context(self.container2, self)
		self.frames[context] = frame
		frame.grid(row=0, column=0, sticky="NSEW")
		self.show_frame(StartPage)

class StartPage(Frame):
	def __init__(self, parent, controller):
		Frame.__init__(self, parent)

		label = Label(self, text="Welcome to the Cloud Pricing App" , font=major_font).grid(row=0, padx=10, pady=10)
		label2 = Label(self, text="Compare Between Amazon, Google, and the University of Michigan\nPlease Make A Selection Below", font=minor_font).grid(row=1, padx=10, pady=10)
		storage_page = Button(self, text="Explore Storage Pricing", font=my_font, padx=20, pady=10, command= lambda: controller.show_frame(StoragePage) ).grid(row=2, padx=10)
		compute_page = Button(self, text="Explore Computing Pricing",font=my_font, padx=10, pady=10, command= lambda: controller.show_frame(ComputingPage) ).grid(row=3, padx=10)

#each page is a seperate class with specific functions.
class StoragePage(Frame):
	def __init__(self, parent, controller):
		Frame.__init__(self, parent)
		self.par_controller = controller

		self.label1 = Label(self, text="Storage Needed (GB)", font=my_font)
		self.label1.grid(row=1, column=0)
		self.entry1 = Entry(self, font=my_font)
		self.entry1.insert(10, "0")
		self.entry1.grid(row=1, column=1, padx=10, pady=10)

		self.label2 = Label(self, text="Read Operations (10,000)", font=my_font)
		self.label2.grid(row=2, column=0)
		self.entry2 = Entry(self, font=my_font)
		self.entry2.insert(10,"0")
		self.entry2.grid(row=2, column=1, padx=10, pady=10)

		self.label3 = Label(self, text="Write Operations (10,000)", font=my_font)
		self.label3.grid(row=3, column=0)
		self.entry3 = Entry(self, font=my_font)
		self.entry3.insert(10, "0")
		self.entry3.grid(row=3, column=1, padx=10, pady=10)

		self.label4 = Label(self, text="Data Scanned (GB)", font=my_font)
		self.label4.grid(row=4, column=0)
		self.entry4 = Entry(self, font=my_font)
		self.entry4.insert(10, "0")
		self.entry4.grid(row=4, column=1, padx=10, pady=10)

		self.label5 = Label(self, text="Data Returned (GB)", font=my_font)
		self.label5.grid(row=5, column=0)
		self.entry5 = Entry(self, font=my_font)
		self.entry5.insert(10, "0")
		self.entry5.grid(row=5, column=1, padx=10, pady=10)

		self.calc_button = Button(self, text="Calculate Final Cost",font=my_font, padx=10, pady=10, command=self.get_input )
		self.calc_button.grid(columnspan=2, sticky="ew", pady=10)


		self.home_button = Button(self, text="Home", font=my_font, padx=1, pady=1, command=self.go_home)
		self.home_button.grid(row=0, column=0, sticky=NW)

	def go_home(self):
		self.par_controller.reset_frame(StoragePage)

	#Takes all the text entry input and puts them into variables
	def get_input(self):
		Application.storage_needed = float(self.entry1.get() )
		Application.read_ops = float(self.entry2.get() )
		Application.write_ops = float(self.entry3.get() )
		Application.data_scan = float(self.entry4.get() )
		Application.data_return = float(self.entry5.get() )

		#Delete entries to make room for the final result
		#I cannot make a new page without the variables updating correctly
		self.entry1.grid_remove()
		self.label1.grid_remove()
		self.entry2.grid_remove()
		self.label2.grid_remove()
		self.entry3.grid_remove()
		self.label3.grid_remove()
		self.entry4.grid_remove()
		self.label4.grid_remove()
		self.entry5.grid_remove()
		self.label5.grid_remove()
		self.calc_button.grid_remove()

		self.label6 = Label(self, text="Final Price Calculations", font=major_font).grid(row=0, column=1, sticky=W)

		#Calculate all of the pricing options
		self.am_s3 = 0.023 * Application.storage_needed + 0.0007 * Application.data_return + 0.002 * Application.data_scan + 0.05 * Application.write_ops + 0.004 * Application.read_ops
		self.am_s3ia = 0.0125 * Application.storage_needed + 0.01 * Application.data_return + 0.002 * Application.data_scan + 0.1 * Application.write_ops + 0.01 * Application.read_ops
		self.am_glac = 0.004 * Application.storage_needed + 0.0025 * Application.data_return + 0.001 * Application.data_scan + 0.5 * Application.write_ops + 0.25 * Application.read_ops

		self.g_mr = 0.026 * Application.storage_needed + 0.05 * Application.write_ops + 0.05 * Application.read_ops
		self.g_near = 0.01 * Application.storage_needed + 0.01 * Application.data_return + 0.1 * Application.write_ops + 0.1 * Application.read_ops
		self.g_cold = 0.007 * Application.storage_needed + 0.05 * Application.data_return + 0.1 * Application.write_ops + 0.1 * Application.read_ops

		self.m_base = 0.0075 * Application.storage_needed
		self.m_bs = 0.009375 * Application.storage_needed
		self.m_br = 0.015 * Application.storage_needed
		self.m_sr = 0.01875 * Application.storage_needed

		#All the Amazon labels and packing
		self.label7 = Label(self, text="Amazon Pricing (per month)", font=minor_font).grid(row=1, column=0)
		self.label8 = Label(self, text="S3 = $" + str(self.am_s3), font=my_font ).grid(row=2, column=1)
		self.label9 = Label(self, text="S3 Infrequent Access = $" + str(self.am_s3ia), font=my_font).grid(row=3, column=1)
		self.label10 = Label(self, text="Glacier = $" + str(self.am_glac), font=my_font).grid(row=4, column=1)

		#All of the Google labels and packing
		self.label11 = Label(self, text="Google Pricing (per month)", font=minor_font).grid(row=5, column=0)
		self.label12 = Label(self, text="Multi-Regional & Regional = $" + str(self.g_mr), font=my_font).grid(row=6, column=1)
		self.label13 = Label(self, text="Nearline = $" + str(self.g_near), font=my_font).grid(row=7, column=1)
		self.label14 = Label(self, text="Coldline = $" + str(self.g_cold), font=my_font).grid(row=8, column=1)

		#All of the Miserver labels and packing
		self.label15 = Label(self, text="MiStorage Pricing (per month)", font=minor_font).grid(row=9, column=0)
		self.label16 = Label(self, text="Base = $" + str(self.m_base), font=my_font).grid(row=10, column=1)
		self.label17 = Label(self, text="Base & Snapshots = $" + str(self.m_bs), font=my_font).grid(row=11, column=1)
		self.label18 = Label(self, text="Base & Replication = $" + str(self.m_br), font=my_font).grid(row=12, column=1)
		self.label19 = Label(self, text="Snapshots & Replication = $" + str(self.m_sr), font=my_font).grid(row=13, column=1)

class ComputingPage(Frame):
	def __init__(self, parent, controller):
		Frame.__init__(self, parent)
		self.con = controller

		self.label1 = Label(self, text="Please Pick An Option", font=major_font)
		self.label1.grid(column=1, pady=10, padx=5, columnspan=2)

		self.button1 = Button(self, text="Pre-Built Computing", font=my_font, padx=10, pady=10, command=self.display_general)
		self.button1.grid(row=1, column=1 ,padx=10, pady=5, columnspan=2)

		self.button3 = Button(self, text="Build Your Own", font=my_font, padx=25, pady=10, command=self.display_build)
		self.button3.grid(row=3, column=1, padx=10, pady=5, columnspan=2)

		self.home_button = Button(self, text="Home", font=my_font, padx=1, pady=1, command=self.go_home)
		self.home_button.grid(row=0, column=0, sticky=NW)

	def go_home(self):
		self.con.reset_frame(ComputingPage)

	def display_general(self):
		self.remove_begin_labels()
		self.drop_menu()


	def display_build(self):
		self.remove_begin_labels()

		self.build_title = Label(self, text="Enter Your Specifications", font=major_font)
		self.build_title.grid(row=0, column=1, columnspan=2)

		self.label2 = Label(self, text="RAM (GB)", font=my_font)
		self.label2.grid(row=1, column=1)
		self.entry2 = Entry(self, font=my_font)
		self.entry2.insert(10, "0")
		self.entry2.grid(row=1, column=2, padx=10, pady=10)

		self.label3 = Label(self, text="Number of CPUs", font=my_font)
		self.label3.grid(row=2, column=1)
		self.entry3 = Entry(self, font=my_font)
		self.entry3.insert(10,"0")
		self.entry3.grid(row=2, column=2, padx=10, pady=10)

		self.label4 = Label(self, text="Memory (GB)", font=my_font)
		self.label4.grid(row=3, column=1)
		self.entry4 = Entry(self, font=my_font)
		self.entry4.insert(10, "0")
		self.entry4.grid(row=3, column=2, padx=10, pady=10)

		self.button4 = Button(self, text="Calculate Price", font=my_font, padx=10, pady=10, command=self.calc_build )
		self.button4.grid(column=1, columnspan=2, sticky="ew", pady=10)

	def calc_build(self):
		self.ram_wanted = float(self.entry2.get())
		self.cpu_wanted = float(self.entry3.get())
		self.storage_wanted = float(self.entry4.get())

		self.label2.grid_remove()
		self.entry2.grid_remove()
		self.label3.grid_remove()
		self.entry3.grid_remove()
		self.label4.grid_remove()
		self.entry4.grid_remove()
		self.button4.grid_remove()
		self.build_title.grid_remove()

		self.calc_title = Label(self, text="Final Price Calculation", font=major_font).grid(row=1, column=0, columnspan=2)
		self.intro = "Prices are based on a machine with: \n" + str(self.ram_wanted) + "(GB) of RAM \n" + str(self.cpu_wanted) + " CPUs\n " + str(self.storage_wanted) + "(GB) of Storage"
		self.into_label = Label(self, text=self.intro, padx=5, pady=5, font=minor_font).grid(row=2, column=0, columnspan=2)

		self.extra_price = 0

		self.miserver_price = 2.295 + 7.25 * self.ram_wanted + 0.0735 * self.storage_wanted

		if (6.5 * self.cpu_wanted) < self.ram_wanted:
			self.extra_ram = self.ram_wanted - (6.5 * self.cpu_wanted)
			self.extra_price = self.extra_ram * 4.88005
			self.ram_wanted -= self.extra_ram

		self.google_price = 16.95 * self.cpu_wanted + 2.35 * self.ram_wanted + 0.04 * self.storage_wanted + self.extra_price
		self.google_title = Label(self, text="Google Compute Engine: ", pady=5, font=my_font).grid(row=3, column=0)
		self.google_p = Label(self, text="$" + str(self.google_price),pady=5, font=my_font).grid(row=3, column=1)

		self.amazon_title = Label(self, text="Amazon EC2: ",pady=5, font=my_font).grid(row=4, column=0)
		self.amazon_p = Label(self, text="Does not offer custom machines", pady=5, font=my_font).grid(row=4, column=1)


		self.mi_title = Label(self, text="MiServer: ", pady=5, font=my_font).grid(row=5, column=0)
		self.mi_p = Label(self, text="$" + str(self.miserver_price), pady=5, font=my_font).grid(row=5, column=1)


	def remove_begin_labels(self):
		self.label1.grid_remove()
		self.button1.grid_remove()
		self.button3.grid_remove()

	def drop_menu(self):
		self.label2 = Label(self, text="Select What You Want To Sort By", font=major_font)
		self.label2.grid(row=0, column=1, padx=10, pady=10)

		self.button7 = Button(self, text="RAM",font=my_font, padx=46, pady=5, command=self.drop_menu_2)
		self.button7.grid(row=1, column=1, padx=10)

		self.button4 = Button(self, text="Number of CPUs",font=my_font, padx=10, pady=5, command=self.drop_menu_3)
		self.button4.grid(row=2, column=1, padx=10)

	def drop_menu_2(self):
		self.label2.grid_remove()
		self.button4.grid_remove()
		self.button7.grid_remove()

		self.label3 = Label(self, text="You have selected RAM", font=minor_font)
		self.label3.grid(row=0, column=1, padx=5, pady=5, columnspan=2)

		self.label33 = Label(self, text="How much is required (GB)?", font=my_font)
		self.label33.grid(row=3, column=1, padx=5, pady=5)

		self.entry1 = Entry(self, font=my_font)
		self.entry1.insert(10, "0")
		self.entry1.grid(row=3, column=2)

		self.label5 = Label(self, text="Additional Storage needed (GB)", font=my_font)
		self.label5.grid(row=4, column=1, padx=5, pady=5)

		self.entry3 = Entry(self, font=my_font)
		self.entry3.insert(10, "0")
		self.entry3.grid(row=4, column=2)

		self.button5 = Button(self, text="Display Options",font=my_font, padx=5, pady=5, command=self.display_ram)
		self.button5.grid(row=5, column=2, padx=10, pady=5)


	def drop_menu_3(self):
		self.label2.grid_remove()
		self.button4.grid_remove()
		self.button7.grid_remove()

		self.label4 = Label(self, text="You have selected CPUs", font=minor_font)
		self.label4.grid(row=0, column=1, padx=5, pady=5, columnspan=2)

		self.label44 = Label(self, text="How many are required?", font=my_font)
		self.label44.grid(row=2, column=1, padx=5, pady=5)

		self.entry2 = Entry(self, font=my_font)
		self.entry2.insert(10, "0")
		self.entry2.grid(row=2, column=2)

		self.label6 = Label(self, text="Additional Storage needed (GB)", font=my_font)
		self.label6.grid(row=3, column=1, padx=5, pady=5)

		self.entry4 = Entry(self, font=my_font)
		self.entry4.insert(10, "0")
		self.entry4.grid(row=3, column=2)

		self.button6 = Button(self, text="Display Options", font=my_font, padx=5, pady=5, command=self.display_cpu)
		self.button6.grid(row=4, column=2, padx=10, pady=5)

	def display_ram(self):
		self.ram_wanted = float(self.entry1.get())
		self.storage_wanted = int(self.entry3.get())

		self.label3.grid_remove()
		self.label33.grid_remove()
		self.entry1.grid_remove()
		self.label5.grid_remove()
		self.entry3.grid_remove()
		self.button5.grid_remove()

		self.google = ""
		self.amazon = ""
		self.miserver = ""
		self.tmp = 0

		self.extra = ""
		self.am_ex1 = ""
		self.g_ex1 = ""
		self.am_ex2 = ""
		self.g_ex2 = ""

		if self.ram_wanted <= 2:
			self.tmp = 24.27 + 0.17 * self.storage_wanted
			self.google = "n1-standard-1, 3.75(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 10.51 + 0.1 * self.storage_wanted
			self.amazon = "t2.small, 2(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 16.795 + 0.0735 * self.storage_wanted
			self.miserver = "server1, 2(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		elif self.ram_wanted <= 4:
			self.tmp = 48.55 + 0.17 * self.storage_wanted
			self.google = "n1-standard-2, 7.5(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 41.98 + 0.1 * self.storage_wanted
			self.amazon = "t2.large, 4(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 31.295 + 0.0735 * self.storage_wanted
			self.miserver = "server2, 4(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		elif self.ram_wanted <= 16:
			self.tmp = 97.09 + 0.17 * self.storage_wanted
			self.google = "n1-standard-4, 15(GB), 4, "  + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 83.95 + 0.1 * self.storage_wanted
			self.amazon = "t2.xlarge, 16(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 118.295 + 0.0735 * self.storage_wanted
			self.miserver = "server3, 16(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.extra = "Higher Memory Options"
			self.am_ex1 = "Amazon EC2: "
			self.g_ex1 = "Google Compute Engine: "

			self.tmp = 122.64 + 0.1 * self.storage_wanted
			self.am_ex2 = "r4.xlarge, 30.5(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 121 + 0.17 * self.storage_wanted
			self.g_ex2 = "n1-highmem-4, 26(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		elif self.ram_wanted <= 31:
			self.google = "Consult higher memory options"
			self.amazon = "Consult higher memory options"
			self.tmp = 7.25 * self.ram_wanted + 2.295 + 0.0735 * self.storage_wanted
			self.miserver = "custom_server, " + str(self.ram_wanted) + "(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.extra = "Higher Memory Options"
			self.am_ex1 = "Amazon EC2: "
			self.g_ex1 = "Google Compute Engine: "

			self.tmp = 122.64 + 0.1 * self.storage_wanted
			self.am_ex2 = "r4.xlarge, 30.5(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 121 + 0.17 * self.storage_wanted
			self.g_ex2 = "n1-highmem-4, 26(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		else:
			self.google = "Consult higher memory options"
			self.amazon = "Consult higher memory options"
			self.tmp = 7.25 * self.ram_wanted + 2.295 + 0.0735 * self.storage_wanted
			self.miserver = "custom_server, " + str(self.ram_wanted) + "(GB), 8, "  + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.extra = "The Highest Memory Options recorded"
			self.am_ex1 = "Amazon EC2: "
			self.g_ex1 = "Google Compute Engine: "

			self.tmp = 245.28 + 0.1 * self.storage_wanted
			self.am_ex2 = "r4.2xlarge, 61(GB), 8, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp) + "/ month"

			self.tmp = 121 + 0.17 * self.storage_wanted
			self.g_ex2 = "n1-highmem-8, 52(GB), 8, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp) + "/ month"


		self.label7 = Label(self, text="Selected Machines Based on RAM Requirement\n[Machine, RAM, # of CPUs, Storage, Cost/month]", font=major_font )
		self.label7.grid(row=0, column=1, padx=10, pady=10, columnspan=2)

		self.label8 = Label(self, text="Google Compute Engine: ", font=my_font)
		self.label8.grid(row=1, column=1, padx=3, pady=7)

		self.label9 = Label(self, text=self.google, font=my_font)
		self.label9.grid(row=1, column=2, padx=3, pady=7)

		self.label10 = Label(self, text="Amazon EC2: ", font=my_font)
		self.label10.grid(row=2, column=1, padx=3, pady=7)

		self.label11 = Label(self, text=self.amazon, font=my_font)
		self.label11.grid(row=2, column=2, padx=3, pady=7)

		self.label12 = Label(self, text="MiServer: ", font=my_font)
		self.label12.grid(row=3, column=1, padx=3, pady=7)

		self.label13 = Label(self, text=self.miserver, font=my_font)
		self.label13.grid(row=3, column=2, padx=3, pady=7)

		self.label14 = Label(self, text=self.extra, font=minor_font)
		self.label14.grid(row=4, column=1, padx=3, pady=10, columnspan=2)

		self.label15 = Label(self, text=self.g_ex1, font=my_font)
		self.label15.grid(row=5, column=1, padx=3, pady=7)

		self.label16 = Label(self, text=self.g_ex2, font=my_font)
		self.label16.grid(row=5, column=2, padx=3, pady=7)

		self.label17 = Label(self, text=self.am_ex1, font=my_font)
		self.label17.grid(row=6, column=1, padx=3, pady=7)

		self.label18 = Label(self, text=self.am_ex2, font=my_font)
		self.label18.grid(row=6, column=2, padx=3, pady=7)


	def display_cpu(self):
		self.cpu_wanted = float(self.entry2.get())
		self.storage_wanted = float(self.entry4.get())

		self.label4.grid_remove()
		self.label44.grid_remove()
		self.entry2.grid_remove()
		self.label6.grid_remove()
		self.entry4.grid_remove()
		self.button6.grid_remove()

		self.google = ""
		self.amazon = ""
		self.miserver = ""
		self.tmp = 0

		self.extra = ""
		self.am_ex1 = ""
		self.g_ex1 = ""
		self.am_ex2 = ""
		self.g_ex2 = ""

		if self.cpu_wanted <= 1:
			self.tmp = 24.27 + 0.17 * self.storage_wanted
			self.google = "n1-standard-1, 3.75(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 10.51 + 0.1 * self.storage_wanted
			self.amazon = "t2.small, 2(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 16.795 + 0.0735 * self.storage_wanted
			self.miserver = "server1, 2(GB), 1, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		elif self.cpu_wanted <= 2:
			self.tmp = 48.55 + 0.17 * self.storage_wanted
			self.google = "n1-standard-2, 7.5(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 41.98 + 0.1 * self.storage_wanted
			self.amazon = "t2.large, 4(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 31.295 + 0.0735 * self.storage_wanted
			self.miserver = "server2, 4(GB), 2, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		elif self.cpu_wanted <= 4:
			self.tmp = 97.09 + 0.17 * self.storage_wanted
			self.google = "n1-standard-4, 15(GB), 4, "  + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 83.95 + 0.1 * self.storage_wanted
			self.amazon = "t2.xlarge, 16(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 118.295 + 0.0735 * self.storage_wanted
			self.miserver = "server3, 16(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.extra = "Higher Memory Options"
			self.am_ex1 = "Amazon EC2: "
			self.g_ex1 = "Google Compute Engine: "

			self.tmp = 122.64 + 0.1 * self.storage_wanted
			self.am_ex2 = "r4.xlarge, 30.5(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 121 + 0.17 * self.storage_wanted
			self.g_ex2 = "n1-highmem-4, 26(GB), 4, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

		else:
			self.google = "Consult higher CPU options"
			self.amazon = "Consult higher CPU options"
			self.tmp = 7.25 * self.ram_wanted + 2.295 + 0.0735 * self.storage_wanted
			self.miserver = "custom_server, " + str(self.ram_wanted) + "(GB), 8, "  + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.extra = "The Highest CPU Options recorded"
			self.am_ex1 = "Amazon EC2: "
			self.g_ex1 = "Google Compute Engine: "

			self.tmp = 245.28 + 0.1 * self.storage_wanted
			self.am_ex2 = "r4.2xlarge, 61(GB), 8, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)

			self.tmp = 121 + 0.17 * self.storage_wanted
			self.g_ex2 = "n1-highmem-8, 52(GB), 8, " + str(self.storage_wanted) + "(GB), $" + str(self.tmp)


		self.label19 = Label(self, text="Selected Machines Based on CPU Requirement\n[Machine, RAM, # of CPUs, Storage, Cost/month]", font=major_font )
		self.label19.grid(row=0, column=1, padx=10, pady=10, columnspa=2)

		self.label20 = Label(self, text="Google Compute Engine: ", font=my_font)
		self.label20.grid(row=1, column=1, padx=3, pady=7)

		self.label21 = Label(self, text=self.google, font=my_font)
		self.label21.grid(row=1, column=2, padx=3, pady=7)

		self.label22 = Label(self, text="Amazon EC2: ", font=my_font)
		self.label22.grid(row=2, column=1, padx=3, pady=7)

		self.label23 = Label(self, text=self.amazon, font=my_font)
		self.label23.grid(row=2, column=2, padx=3, pady=7)

		self.label24 = Label(self, text="MiServer: ", font=my_font)
		self.label24.grid(row=3, column=1, padx=3, pady=7)

		self.label25 = Label(self, text=self.miserver, font=my_font)
		self.label25.grid(row=3, column=2, padx=3, pady=7)

		self.label26 = Label(self, text=self.extra, font=minor_font)
		self.label26.grid(row=4, column=1, padx=3, pady=10, columnspan=2)

		self.label27 = Label(self, text=self.g_ex1, font=my_font)
		self.label27.grid(row=5, column=1, padx=3, pady=7)

		self.label28 = Label(self, text=self.g_ex2, font=my_font)
		self.label28.grid(row=5, column=2, padx=3, pady=7)

		self.label29 = Label(self, text=self.am_ex1, font=my_font)
		self.label29.grid(row=6, column=1, padx=3, pady=7)

		self.label30 = Label(self, text=self.am_ex2, font=my_font)
		self.label30.grid(row=6, column=2, padx=3, pady=7)

app = Application()
